{
  "name": "newscreen-client",
  "description": "newscreen-client",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "livereload": "livereload ./src",
    "dev:https": "node src/httpsServer.js",
    "dev:debug:https": "NODE_OPTIONS='--inspect' npm run dev:https",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "analyze:build": "ANALYZE=true npm run build",
    "start": "next start",
    "jsconfig:generate": "node jsconfig:generate",
    "get-schema": "rimraf .schema.graphql && graphql codegen",
    "relay-compiler": "relay-compiler",
    "postinstall": "npm run jsconfig:generate && npm run get-schema && npm run relay-compiler",
    "styleguide:build": "styleguidist build",
    "eslint:fix": "eslint . --fix",
    "prepare": "husky install",
    "test": "lint-staged"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.0.0",
    "@hookform/resolvers": "^2.5.2",
    "@next/bundle-analyzer": "^10.2.3",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.0",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "graphql-config": "^3.3.0",
    "https-localhost": "^4.6.5",
    "lodash": "^4.17.20",
    "next": "^11.0.0",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-cookie-consent": "^6.2.4",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.7.1",
    "react-relay": "^11.0.2",
    "react-relay-network-modern": "^6.0.0",
    "react-relay-network-modern-ssr": "^1.4.0",
    "react-spring": "^9.2.1",
    "react-use-wizard": "^1.1.2",
    "relay-hooks": "^4.2",
    "relay-runtime": "^11.0.2",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "twin.macro": "^2.4.2",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-react": "^7.12.10",
    "@graphql-cli/codegen": "^1.17.24",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^11.0.2",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-nextjs": "^1.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "graphql": "^15.4.0",
    "graphql-cli": "^4.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "livereload": "^0.9.3",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "relay-compiler": "^11.0.2",
    "relay-config": "^11.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.2"
  },
  "lint-staged": {
    "*": [
      "npm run relay-compiler"
    ],
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix"
    ]
  },
  "engines": {
    "node": "14.16.1",
    "npm": "6.14.12"
  }
}
